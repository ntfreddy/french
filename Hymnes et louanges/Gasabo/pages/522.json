	"lyrics": [
		{"label":"1",
			"lines":[
				"Ne crains rien, je t'aime,",
				"Je suis avec toi!",
				"Promesse suprême,",
				"Qui soutient ma foi.",
				"La sombre vallée",
				"N'a plus de terreur,",
				"L'âme consolée,",
				"Je marche avec mon Sauveur."
			]
		},
		{"label":"Choeur",
			"lines":[
				"Non, jamais tout seul  {2 fois",
				"Jésus, mon Sauveur, me garde,",
				"Jamais ne me laisse seul.",
				"Non, jamais tout seul  {2 fois",
				"Jésus, mon Sauveur, me garde,",
				"Je ne suis jamais tout seul."
			]
		},
		{"label":"2",
			"lines":[
				"L'aube matinière",
				"Ne luit qu'aux beaux jours,",
				"Jésus, ma lumière,",
				"M'éclaire toujours.",
				"Quand je perds de vue",
				"L'astre radieux,",
				"À travers la nue,",
				"Jésus me montre les cieux!"
			]
		},
		{"label":"3",
			"lines":[
				"Les dangers accourent,",
				"Subtils, inconnus :",
				"De près ils m'entourent,",
				"Plus près est Jésus,",
				"Qui, dans le voyage,",
				"Me redit : « C'est moi!",
				"Ne crains rien : courage!",
				"Je suis toujours avec toi!»"
			]
		}
	]
}

